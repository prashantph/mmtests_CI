# MM Test Parameters
export MMTESTS="iperf3"

export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_UPDATE_FREQUENCY=10

# Iperf3
export IPERF3_PROTOCOL=tcp
export IPERF3_MESSAGE_SIZE=14
export IPERF3_MESSAGE_RATE=10000
export IPERF3_MIN_CLIENTS=1
export IPERF3_MAX_CLIENTS=$NUMCPUS
export IPERF3_DURATION=30
